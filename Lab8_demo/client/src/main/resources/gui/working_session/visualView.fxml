<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="visualizationPane" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/23.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="gui.working_session.VisualizationViewController">
    <ComboBox fx:id="languageComboBox"
              prefHeight="24.0" prefWidth="130.0"
              AnchorPane.topAnchor="10.0" AnchorPane.rightAnchor="50.0" onAction="#handleLanguageClicked"/>
    <Button fx:id="insertButton"
            mnemonicParsing="false" text="+"
            style="-fx-background-radius: 5em; -fx-min-width: 33px; -fx-min-height: 33px; -fx-max-width: 33px; -fx-max-height: 33px;"
            AnchorPane.topAnchor="8.0" AnchorPane.rightAnchor="10.0" onAction="#handleInsertClicked"/>
</AnchorPane>

