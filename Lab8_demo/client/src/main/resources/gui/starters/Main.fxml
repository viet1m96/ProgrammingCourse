<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.layout.HBox?>
<AnchorPane prefHeight="500.0" prefWidth="800" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.starters.MainApplication">
   <VBox alignment="CENTER" prefHeight="500.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <Label fx:id="welcomeLabel" alignment="CENTER" textAlignment="CENTER">
         <font>
            <Font size="38.0" />
         </font>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </Label>
      <HBox alignment="CENTER">
         <ComboBox fx:id="languageComboBox" prefWidth="150.0" onAction="#handleLanguageClicked" />
         <Button fx:id="continueButton" mnemonicParsing="false" onAction="#continueButtonClicked" text="Continue">
            <HBox.margin>
               <Insets left="20.0" />
            </HBox.margin>
         </Button>
      </HBox>
   </VBox>
</AnchorPane>
