<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="800" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.starters.ChoicesSteps">
    <VBox alignment="CENTER" layoutY="64.0" prefHeight="436.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <Label fx:id="welcomeLabel" alignment="CENTER" textAlignment="CENTER">
            <font>
                <Font size="38.0" />
            </font>
            <VBox.margin>
                <Insets bottom="20.0" />
            </VBox.margin>
        </Label>
        <HBox alignment="CENTER" spacing="100.0">
            <Button fx:id="SignInButton" mnemonicParsing="false" onAction="#SignInButtonClicked" />
            <Button fx:id="SignUpButton" mnemonicParsing="false" onAction="#SignUpButtonClicked"  />
        </HBox>
    </VBox>
    <Button fx:id="BackButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#BackButtonClicked" prefHeight="31.0" prefWidth="31.0" style="-fx-background-radius: 15;" />
    <ImageView fx:id="BackButtonImage" fitHeight="32.0" fitWidth="31.0" layoutX="14.0" layoutY="14.0" mouseTransparent="true" pickOnBounds="true" preserveRatio="true">

    </ImageView>
</AnchorPane>
