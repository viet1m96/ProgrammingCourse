<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="500.0" prefWidth="800" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.sign_in_up.SignInController">
   <BorderPane prefHeight="500.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <center>
         <VBox alignment="CENTER" prefWidth="400.0" spacing="20.0">
            <padding>
               <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
            <VBox alignment="BASELINE_LEFT" spacing="20.0">
               <TextField fx:id="usernameField" promptText="username" prefWidth="350.0"/>
               <Text fx:id="criteriaUsername" text="✓ Not be empty" />
            </VBox>
            <VBox alignment="BASELINE_LEFT" spacing="20.0">
               <PasswordField fx:id="passwordField" promptText="password" prefWidth="350.0"/>
               <Text fx:id="criteriaPassword" text="✓ Not be empty" />
            </VBox>
            <Button fx:id="signInSubmitButton" mnemonicParsing="false" text="Sign In" onAction="#SignInButtonClicked"/>
         </VBox>
      </center>
   </BorderPane>
   <Button fx:id="BackButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#BackButtonClicked" prefHeight="31.0" prefWidth="31.0" style="-fx-background-radius: 15;" />
   <ImageView fx:id="BackButtonImage" fitHeight="32.0" fitWidth="31.0" layoutX="14.0" layoutY="14.0" mouseTransparent="true" pickOnBounds="true" preserveRatio="true">

   </ImageView>
</AnchorPane>
